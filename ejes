          ansiErrOut(ERROR + 'Invalid argument: "' + ITEM + '<required value omitted>' + ERROR + '", Option: ' + ERROR + cmd.long + ERROR + RESET)
          cmd.doList && cmd.doList(cmd.long.substring(2))
          term.exitNow(23, true)
        }
      }
